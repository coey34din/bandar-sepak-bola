<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title or metadata.title }}</title>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

  <link rel="apple-touch-icon" sizes="180x180" href="{{ '/' | url }}static/img/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ '/' | url }}static/img/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ '/' | url }}static/img/favicon-16x16.png">
  <link rel="manifest" href="{{ '/' | url }}static/img/site.webmanifest">
  <link rel="mask-icon" href="{{ '/' | url }}static/img/safari-pinned-tab.svg" color="#5bbad5">
  <meta name="msapplication-TileColor" content="#da532c">
  <meta name="theme-color" content="#ffffff">
  {% set css %}
    {% include "assets/css/bootstrap.min.css" %}    
    {% include "assets/css/inline.css" %}    
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>

  {% set js %}
    {% include "assets/js//bootstrap.min.js" %} 
    {% include "assets/js/inline.js" %} 
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
</head>
