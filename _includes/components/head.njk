<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title or metadata.title }}</title>
  <link rel='stylesheet' href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/5.8.1/css/all.css">
  <script async src="//identity.netlify.com/v1/netlify-identity-widget.js"></script>
  <script async src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
  <script async src="//cdnjs.cloudflare.com/ajax/libs/jquery-migrate/3.0.1/jquery-migrate.js"></script>
  {% set css %}
    {% include "assets/css/bulma.css" %}
    {% include "assets/css/inline.css" %}
    {% include "assets/css/flexslider.css" %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>

  {% set js %}
    {% include "assets/js/inline.js" %} 
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
</head>
